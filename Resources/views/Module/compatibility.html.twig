{{ adminHeader() }}
<h3>
    <span class="fa fa-info-circle"></span>
    {% trans %}Incompatible version with the core{% endtrans %} - {{ extension.displayname }}
    {{ pageSetVar('title', __('Incompatible version')) }}
</h3>
<div class="form-group">
    <p><strong>{% trans %}The version of this module is incompatible with the version of the core.{% endtrans %}</strong></p>
    <ul>
        <li>{{ __f('Current Core version is %version%',
            {'%version%': '<span class="badge badge-success">' ~ pagevars.coredata.version ~ '</span>'})|raw }}</li>
        {% if extension.coreCompatibility is not empty %}
        <li>{{ __f('The minimum version of the core that this module supports is %s', {"%s": '<span class="badge badge-warning">' ~ extension.coreCompatibility ~ '</span>'})|raw }}</li>
        {% endif %}
    </ul>
</div>
<div class="form-group">
    <div class="col-md-9 offset-md-3">
        <a class="btn btn-primary" href="{{ path('zikulaextensionsmodule_module_viewmodulelist') }}"><i class="fa fa-check"></i> {% trans %}Ok{% endtrans %}</a>
    </div>
</div>
{{ adminFooter() }}
